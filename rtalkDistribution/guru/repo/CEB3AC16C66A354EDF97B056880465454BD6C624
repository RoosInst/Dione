                                                                                                 " keys are the time stamps and values are the class and method names "                                                                                                                                      | sym dict stm tmp dateRef tMethod tClass tStamp lclOffset|                                                                                                                                      dict := Dictionary new.                                                                                                                                  	dateRef:= Date new.                                                                                                                                  	lclOffset := Date localTimeOffset.                                                                                                                                      instanceMethods associationsDo:[ :anAssociation |                                                                                                                                  		tClass := (RtClass getClass: anAssociation key).                                                                                                                                          anAssociation value do:[ :sym |                                                                                                                                  			tMethod := tClass compiledMethodAt:sym.                                                                                                                                  			dateRef gmtSeconds:(tMethod timeStamp + lclOffset).                                                                                                                                  			[tStamp := dateRef timestamp.                                                                                                                                  			dict includesKey:tStamp] whileTrue:[ dateRef secondsInDay:(dateRef secondsInDay + 1)].                                                                                                                                  			dict at:tStamp put:(tClass name,' - ',(tMethod sourceString))                                                                                                                                  			]].                                                                                                                                      classMethods associationsDo:[ :anAssociation |                                                                                                                                  		tClass := (RtClass getClass: anAssociation key) class.                                                                                                                                          anAssociation value do:[ :sym |                                                                                                                                  			tMethod := tClass compiledMethodAt:sym.                                                                                                                                  			dateRef gmtSeconds:(tMethod timeStamp + lclOffset).                                                                                                                                  			[tStamp := dateRef timestamp.                                                                                                                                  			dict includesKey:tStamp] whileTrue:[ dateRef secondsInDay:(dateRef secondsInDay + 1)].                                                                                                                                  			dict at:tStamp put:(tClass name,' - ',(tMethod sourceString))                                                                                 