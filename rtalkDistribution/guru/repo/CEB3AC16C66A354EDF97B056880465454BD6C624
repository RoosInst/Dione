                                                                                                       | rtn tmp current np|                                                                                                                                      rtn := RiCffParagraphTree new initialize.                                                                                                                                      current := rtn.                                                                                                                                      self reset.                                                                                                                                      self nextLine.  " get past the time stamp "                                                                                                                                      [self atEnd] whileFalse:[                                                                                                                                             (self peek = $:)                                                                                                                                            ifTrue:[                                                                                                                                              tmp := (self nextLine) trimBlanks riMapped.                                                                                                                                              ((tmp first at:2) = $:)  " this is a push down :: "                                                                                                                                                  ifTrue:[               np := RiCffParagraphTree new initialize parent:current.                                                                                                                                                                      np name:(tmp first).                                                                                                                                                                      np addAttributes:tmp.                                                                                                                                                                      current addParagraph:np.                                                                                                                                                                      current := np.]                                                                                                                                                  ifFalse:[                                                                                                                                                    tmp first = ':CHECKSUM'  ifTrue:[[self atEnd] whileFalse:[self nextLine]. ^rtn].                                                                                                                                                    tmp first = ':END'  " this is a pop up "                                                                                                                                                      ifTrue:[current := current parent]                                                                                                                                                      ifFalse:[ " this is another paragraph to add to current parent"                                                                                                                                                                       " add a new child "                                                                                                                                                                      np := RiCffParagraphTree new initialize parent:current.                                                                                                                                                                      np name:(tmp first).                                                                                                                                                                      np addAttributes:tmp.                                                                                                                                                                      current parent addParagraph:np.                                                                                                                                                                      current := np.                                                                                                                                                                      ]]]                                                                                                                                              ifFalse:[current addLine:(self nextLine)].  " gets a line of stuff into items "                                                                                                                                                                                                                                                                                ].                                                                                                                                                                                                                                                                         self validateChecksum;close.                                                                                                                                      ^rtn                                         :SOURCE,type=instanceMethod,class=RicffFileStream,selector=readInfoSection,codeType=rtalkreadInfoSection                    