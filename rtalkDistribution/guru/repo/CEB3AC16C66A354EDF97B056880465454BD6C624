                                                                                                                                                                                                          " now do the diff "                                                                                                                                      old  := ReadStream on:aRefString.                                                                                                                                      current := ReadStream on:aString.                                                                                                                                      diff := OrderedCollection new.                                                                                                                                      colors := OrderedCollection new.                                                                                                                                      diff add:'Lines in old and current'.                                                                                                                                  	colors add:4.                                                                                                                                      diff add:'Lines in current but not in old'.	                                                                                                                                  	colors add:5.                                                                                                                                      diff add:'Lines in old and not in current'.                                                                                                                                  	colors add:2.                                                                                                                                      cLines := OrderedCollection new.                                                                                                                                      [current atEnd] whileFalse:[tmp := current nextLine.                                                                                                                                                                        tmp isEmpty ifFalse:[ cLines add:tmp]].                                                                                                                                      oLines := OrderedCollection new.                                                                                                                                      [old atEnd] whileFalse:[tmp := old nextLine.                                                                                                                                                                  tmp isEmpty ifFalse:[oLines add:tmp]].                                                                                                                                      cLine := 1.                                                                                                                                      oLine := 1.                                                                                                                                      [(cLine <= cLines size) & (oLine <= oLines size)] whileTrue:[                                                                                                                                      (((cLines at:cLine) trimBlanks) = ((oLines at:oLine) trimBlanks))                                                                                                                                              ifTrue:[diff add:(cLines at:cLine).                                                                                                                                    						colors add:4.                                                                                                                                                        cLine := cLine + 1.                                                                                                                                                      oLine := oLine + 1]                                                                                                                                              ifFalse:[ " find the next match of any following line in c into o "                                                                                                                                                      cTmp := cLine.                                                                                                                                                      oTmp := oLine.                                                                                                                                                      cMatch := nil.                                                                                                                                                      [ (cTmp <= cLines size) & (cMatch isNil)] whileTrue:[                                                                                                                                                          oTmp := oLine.                                                                                                                                                          [(oTmp <= oLines size)  & (cMatch isNil)] whileTrue:[                                                                                                                                                                  (((cLines at:cTmp)trimBlanks) = ((oLines at:oTmp)trimBlanks))                                                                                                                                                                      