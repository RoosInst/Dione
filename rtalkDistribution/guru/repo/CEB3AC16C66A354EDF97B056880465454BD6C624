:$=.                                                                                                                                                            aGuruFile nextPutAll:(a value asString)]]].                                                                                                                                                      multilines do:[ :m | m value writeToCff:aGuruFile]].                                                                                                                                                                                                                                                                        self parent ifRoot                                                                                                                                          ifFalse:[aGuruFile cr; nextPutAll:':END'.]                                         :SOURCE,type=instanceMethod,class=RiCffParagraphTree,selector=associations,codeType=rtalkassociations                                                                                                                                     "<modified:user=Roos Inst,time=11/01/08 at 06:44:21 pm>   "                                                                                                                                     "<modified:user=Roos Inst,time=11/01/08 at 06:43:29 pm>   "                                                                                                                                     "<modified:user=Roos Inst,time=11/01/08 at 06:42:11 pm>   "                                                                                                                                          "returns collection of associations with key as child name"                                                                                                                                         ^children associations                                         :SOUR