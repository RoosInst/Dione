 if OK, aString if not"                                                                                                                                  	| nSize tmp vars|                                                                                                                   	self hasInstances ifTrue:[^self name,' has instances '].                                                                                                                  	self subclasses do:[ :sc | sc hasInstances ifTrue:[ sc name,' has instances ']].                                                                                                                                    	" get a list of the vars "                                                                                                                                  	vars := aString asArrayOfSubstrings.                                                                                                                                  	" first char is lower case "                                                                                                                                  	vars do:[ :var |                                                                                                                                  		((var at:1) isLowerCase) ifFalse:[^'must start with lower case'].                                                                                                                                  	" check that its not a duplicate "                                                                                                                                  		(self allInstVarNames includes:var) ifTrue:[^'duplicate var name'].                                                                                                                                  		self withAllSubclasses do:[ :sc |                                                                                                                                  			(sc instVarNames includes:var)                                                                                                                                  				ifTrue:[^'instance var duplicated in subclass']]].                                                                                                                                  	" add the var to the list of names and call the prim to change the shape "                                                                                                                                  	tmp := instances.                                                                                                                                   	instances := Array new:(instances size + vars size).                                                                                                                                  	instances replaceFrom:1 to: (tmp size) with: tmp.                                                                                                                                  	instances replaceFrom:(tmp size + 1) to: (instances size) with: vars.                                                                                                                                  	self computeInstSize.                                                                                                                                  	self timeStamp:(RiGuruTime secondsSince1990GMT).                                                                                                                   	self subclasses do:[ :sc | sc computeInstSize].                                                                                                                                	^true                         :SOURCE,type=instanceMethod,class=RtClass,selector=structureString,codeType=rtalkstructureString                                                                                                                                     "<modified:pgmr=mdr,time=08/15/12 at 08:46:29 am>   "                                                                                                                                          "short string for structure"                                                                                                                                  	self isJavaObject ifTrue:[^'java'].                                                                                                                                  	self isFloatArray ifTrue:[^'floatArray'].                                                                                                                                  	self isInteger ifTrue:[^'integer'].                                                                                                                                  	self isFloat ifTrue:[^'float'].                                                                                                                                  	self isFixed                                                                                                                                              ifTrue:[^'named']                                                                                                                                          ifFalse:[self isBytes                                                                                                                                                          ifTrue:[^'bytes']                                                                                                                                                      ifFalse:[    self isPointers                                                                                                                                                           ifTrue:[^'indexed']]].                                                  