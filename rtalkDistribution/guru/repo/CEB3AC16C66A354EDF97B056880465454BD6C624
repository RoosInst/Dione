        pos = next size  ifTrue:[ self nextLine. ^next copyFrom:1 to:(pos - 1)].                                                                                                                                          pos = 0                                                                                                                                              ifTrue:[self nextLine.                                                                                                                                                        ^ next].                                                                                                                                          rtn := next copyFrom:1 to:(pos - 1).                                                                                                                                          nextLine := next copyFrom:(pos + 1) to:(next size).                                                                                                                                          ^rtn                                                                                                                                                                                                                                                                        ].                                                                                                                                      ^nil                                                                               :SOURCE,type=instanceMethod,class=RtCffFile,selector=guruAttributes:,codeType=rtalkguruAttributes:aDict                                                                                                                                          "used by guru to copy"                                                                                                                                      file guruAttributes:aDict                          