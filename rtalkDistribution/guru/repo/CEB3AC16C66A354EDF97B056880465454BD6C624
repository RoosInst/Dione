                                                         "returns a RiCffMappedCollection"                                                                               	|  start size|                                                                               	start := 1.                                                                               	size := aString size.                                                                              	1 to:size do:[ :pos |                                                                               		((aString at:pos) = $,)                                                                               			ifTrue:[ aMap add:((aString copyFrom:start to:(pos - 1))riKeyValue ).                                                                               					start := pos + 1]                                                                               		].                                                                               	aMap add:((aString copyFrom:start to:size)riKeyValue ).                                                                              		                                                                                   ^aMap                                                                              :SOURCE,type=instanceMethod,class=RtCffFile,selector=writeTimeStamp,codeType=rtalkwriteTimeStamp                                                                                                                                          "call first to insert a leading time stamp"                                                                                                                                                                                                                                                                             self nextPutAll:(RiGuruTime guruTime);cr.                                         :SOURCE,type=instanceMethod,class=RtCffFile,selector=next