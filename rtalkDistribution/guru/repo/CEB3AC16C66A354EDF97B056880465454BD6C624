ue:[prereqs := OrderedCollection new].                                                                                                                                                          tmp := a multiLinesDo:[ :c |                                                                                                                                                                  prereqs add:(c riAtKey:'rid')]].                                                                                                                                          ].                                                                                                                                      number := self name fileExtension.                                                                                                                                      date := guruAttributes creationDate                                         :SOURCE,type=instanceMethod,class=RtCodeDef,selector=becomeCandidate,codeType=rtalkbecomeCandidate                                                                                                                                     "<modified:sys=GAKRE8CA,time=12/16/11 at 08:34:07 pm>   "                                                                                                                                  "                                                                                                                                  "                                                                                                                                      guruAttributes status:'beta'                                         :SOURCE,type=instanceMethod,class=RtCodeDef,selector=ifLastRevision,codeType=rtalkifLastRevision                                                                                                                                     "<modified:pgmr=mdr,time=06/11/14 at 07:43:13 pm>   "                   