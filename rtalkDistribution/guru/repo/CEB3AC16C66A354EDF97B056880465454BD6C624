p histTag pkgTime tClass tMethod|                                                                                                                                  	histTag := self title.                                                                                                                                  	pkgTime = guruAttributes rootTimeSeconds.                                                                             	sourceTree isNil                                                                             		ifTrue:[self riLogMsg:'Missing code for ',self title]                                                                                       		ifFalse:[self riLogMsg:'Compiling package ',self title].                                                                                                                                    sourceTree paragraphsDo:[ :a |                                                                                                                                          a name = 'CLASSES'                                                                                                                                               ifTrue:[tmp := a multiLinesDo:[ :c |                                                                                                                                                          tClass := self addClassFrom:c.                                                                                             			tClass isNil ifFalse:[                                                                                                                                                         	 	tClass timeStamp:pkgTime.                                                                                                                                                          		c riIncludesKey:'modified' ifTrue:[ :t | tClass timeStamp:(t asInteger)].                                                                                                                                                          		c riIncludesKey:'category' ifTrue:[ :t | tClass category:(t asSymbol)]]]].                                                                                                                                          a name = 'METHODS'                                                                                                                                               ifTrue:[tmp := a multiLinesDo:[ :c |                                                                                                                                                          self addMethodFrom:c.                                                                                                                                                          ]].                                                                                                                                          a name = 'SOURCE'                                                                                                                                               ifTrue:[ self addSource:(a contentsAsString) toMethod:(a attributes)].                                                                                                                                          a name = 'CODE'                                                                                                                                               ifTrue:[                                                                                                                                                  tmp := a attributes riAtKey:'selector'.                                                                                                                                                  tmp isNil                                                                                                                                                      ifFalse:[ (RtClass classExists:((a attributes riAtKey:'rtClass')asSymbol))                                                                                                                                                          ifTrue:[ tMethod := self compileCode:(a contentsAsString) attributes:(a attributes).                                                                                                                                                                  	tMethod isNil                                                                                                                                  										ifTrue:[self riLogMsg:('pkg ',(self name),' Compile Fail ',(a attributes riAtKey:'rtClass'),' ',tmp).]                                                                                                                                  										ifFalse:[		tMethod timeStamp:pkgTime.                                                                                                                                                                              			a attributes riIncludesKey:'modified' ifTrue:[ :t | tMethod timeStamp:(t asInteger)].                                                                                                                                                                              			a attributes riIncludesKey:'category' ifTrue:[ :t | tMethod category:(t asSymbol)]]]                                                                                                                                                          ifFalse:[ self riLogMsg:('Pkg ',(self name),' Missing Class ',(a attributes riAtKey:'rtClass')).                                                                                                                                                                          ]]]].                                                                             :SOURCE,type=instanceMethod,class=RtCodeDef,selector=creationDate,codeType=rtalkcreationDate                                                                                                                                          "comment"                                                                                                                                      ^guruAttributes creationDate                                         :SOURCE,type=instanceMethod,class=RtCodeDef,selector=hasChanged,codeType=rtalkhasChanged                                                                                                                                          "Answer aBoolean, true if the list of what the library contains has                                                                                                                                            changed sence the last time it was saved, elaes fales."                                                                                                                                      |  |                                                                                                                                      ^changed                                         :SOURCE,type=instanceMethod,class=RtCodeDef,selector=subVersion,codeType=rtalksubVersion                                                                                                                                     "<modified:sys=GAKRE8CA,time=04/09/12 at 10:34:25 am>   "                                                                                                                                     "<modified:sys=GAKRE8CA,time=04/09/12 at 09:17:43 am>   "                                                                                                                                          "incremented on each save"                                                                                                                                      | rtn |                                                                                                                                      rtn := guruAttributes subVersion.                                                                                                                                      rtn isEmpty                                                                                                                                          ifTrue:[^0]                                                                                                                                          ifFalse:[^rtn asInteger]                                         :SOURCE,type=instanceMethod,class=RtCodeDef,selector=loadSchemaWithAttr:,codeType=rtalkloadSchemaWithAttr:aAttr                                                                                                                                     "<modified:pgmr=mdr,time=06/13/14 at 08:49:06 pm>   "                                                                                 "load based on attributes"                                                                                                                                      | gf rtn attr tmp tClass tMethod sel pkgTime cName|                                                                                                                                      aAttr isNil ifTrue:[^nil].                                                                                                                                      attr := aAttr.                                                                                                                                      gf := RiGuru openObject: attr.                                                                                                                                      (gf isNil not and:[ gf isEmpty not])                                                                                          