                                                                                               high := aInt // 65536.                                                                                                                                      index := size.                                                                                                                                      self at: index  put: (low \\ 256).                                                                                                                                      size < 2 ifTrue:[^self].                                                                                                                                      self at: (index - 1) put: (low // 256).                                                                                                                                      size < 3 ifTrue:[^self].                                                                                                                                      self at: (index - 2) put: (high \\ 256).                                                                                                                                      size < 4 ifTrue:[^self].                                                                                                                                      self at: (index - 3) put: (high // 256 \\ 256).                                         :SOURCE,type=instanceMethod,class=ByteArray,selector=riAt:putWord:,codeType=rtalkriAt:aInt putWord:aInt2                                                                                                                                          "put the word  starting at aInt"                                                                                                                                   | low high off|                                                                                                                                      low := aInt2 rounded \\ 65536.                                                                                                                                      off:= aInt.                                                                                                                                      self  at: off       put: (low \\ 256).                                                                                                                                      self  at: (off + 1) put: (low // 256).                                         :SOURCE,type=instanceMethod,class=ByteArray,selector=riByteAt:,codeType=rtalkriByteAt:aInt                                                                                                                                           "get the long  starting at aInt"                                                                                                                                      ^self at:aInt                                         :SOURCE,type=instanceMethod,class=ByteArray,selector=riAt:putLong:,codeType=rtalkriAt:aInt putLong:aInt2                                                                                                                                          "put the long  starting at aInt"                                                                                                                                   | low high off|                                                                                                                                      low := aInt2 rounded \\ 65536.                                                                                                                                      high := aInt2 rounded  // 65536.                                                                                                                                      off:= aInt.                                      