                                                                     ifTrue:[ tmp := char digitValue * 16]                                                                                                                                              ifFalse:[ tmp := char asciiValue - 55 * 16].                                                                                                                                          char := src next.                                                                                                                                          char isDigit                                                                                                                                              ifTrue:[ tmp := tmp + char digitValue]                                                                                                                                              ifFalse:[ tmp := tmp + char asciiValue - 55].                                                                                                                                          array at:index put:tmp.                                                                                                                                          index := index + 1].                                                                                                                                      index := 1.                                                                                                                                      rtn := 0.                                                                                                                                      array size timesRepeat:[                                                                                                                                          rtn := rtn * 256 + (array at:index).                                                                                                                                          index := index + 1].                                                                                                                                      ^rtn                                         :SOURCE,type=instanceMethod,class=String,selector=riSubstitute:for:,codeType=rtalkriSubstitute:aChar for:aChar2                                                                                                                                          "replace chars"                                                                                                                                                                                                                                                                            1 to:self size do:[ :i |                                                                                                                                              (self at:i) = aChar2                                                                                                                                                  ifTrue:[self at:i put:aChar]]                                         :SOURCE,type=instanceMethod,class=String,selector=riFirstSubstringSepBy:,codeType=rtalkriFirstSubstringSepBy:aChar                                                                                                                                          "Answer an  substrings from the                                                                                                                                           receiver.  The receiver is divided into                                                                                                                                           substrings at the occurrences aChar"                                                                                                                                  " JB 000218  Changed to use ReadS