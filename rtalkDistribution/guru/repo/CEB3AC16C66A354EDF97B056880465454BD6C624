                             		(classes includes:cn) ifFalse:[ extends add:cn]].                                                                                                                                  	classes do:[ :cn |                                                                                                                                  		aClass := RtClass getClass:cn.                                                                                                                                  		sClass := aClass superclass.                                                                                                                                  		(sClass isNil or:[ sClass == Object ])                                                                                                                                  			ifFalse:[                                                                                                                                  				(classes includes:(sClass symbol))                                                                                                                                  					ifFalse:[extends add:cn]]].                                                                                                                                      extends isEmpty                                                                                                                                              ifFalse:[aFile cr; nextPutAll:':EXTENDS'.                                                                                                                                                          extends do:[ :aSymbol |                                                                                                                                                              aClass := RtClass getClass: aSymbol.                                                                                                                                                              self fileOutClassV2:aClass to: aFile].                                                                                                                                                  ].                                                                                                                                      classes isEmpty                                                                                                                                              ifFalse:[aFile cr; nextPutAll:':CLASSES'.                                                                                                                                                  (classes asSortedCollection:[ :a :b |                                                                                                                                                      ((RtClass getClass: a) hierarchyLevel) <=                                                                                                                                                              ((RtClass getClass: b) hierarchyLevel)])                                                                                                                                                          do:[ :aSymbol |                                                                                                                                                              aClass := RtClass getClass: aSymbol.                                                                                                                                                              self fileOutClassV2:aClass to: aFile].                                                                                                                                                  ].                                                                                                                                      ((classMethods isEmpty) & ( instanceMethods isEmpty))                                                                                                                                          ifFalse:[                                                                                                                                              aFile cr; nextPutAll:':METHODS'.                                                                                                                                              classMethods associationsDo:[ :anAssociation |                                                                                                                                                  aClass := (RtClass getClass: anAssociation key) class.                                                                                                                                                  anAssociation value do:[ :aSymbol |                                                                                                                                  					method := aClass compiledMethodAt:aSymbol.                                                                                                                                                      aFile cr;                                                                                                                                                           nextPutAll:'class,class=',(aClass symbol asString),',selector=',(aSymbol asString riMap),',codeType=rtalk'.                                                                                                                                           			method category isNil ifFalse:[aFile comma;nextPutAll:'category=',(method category asString)].                                                                                                                                                                			 method timeStamp = 0                                                                                                                                   					ifFalse:[aFile comma;nextPutAll:'modified=',(method timeStamp asString)].                                                                                                                                          		method pbcBytes isNil                                                                                                                                              			ifFalse:[   aFile comma;cr;nextPutAll:'pbc=',(method pbcBytes asHexString)].                                                                                                                                  					]].                                                                                                                                                                                                                                                                                instanceMethods associationsDo:[ :anAssociation |                                                                                                                                                  aClass := (RtClass getClass: anAssociation key).                                                                                                                                                  anAssociation value do:[ :aSymbol |                                                                                                                                  					method := aClass compiledMethodAt:aSymbol.                                                                                                                                                      aFile cr;                                                                                                                                                           nextPutAll:'instance,class=',(aClass symbol asString),',selector=',(aSymbol asString riMap),'