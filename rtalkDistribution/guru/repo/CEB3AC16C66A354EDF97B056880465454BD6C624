                               | dirLoc file path fileLoc|                                                                                                                                      " find the drive "                                                                                                                                      dirLoc := aString indexOf:$:.                                                                                                                                      dirLoc ~= 0 ifTrue:[self at:'RI.SYS.FILEDIR' put:(aString copyFrom:1 to:dirLoc)].                                                                                                                                      " now the file "                                                                                                                                      fileLoc:= aString indexFromLastOf:$\.                                                                                                                                      fileLoc = 0                                                                                                                                          ifTrue:[dirLoc = 0                                                                                                                                                      ifTrue:[^self fileNameWithExtension:aString]                                                                                                                                                      ifFalse:[^self fileNameWithExtension:(aString copyFrom:(dirLoc + 1) to:(aString size))]]                                                                                                                                          ifFalse:[dirLoc = 0                                                                                                                                                      ifTrue:[path := aString copyFrom:1 to:f