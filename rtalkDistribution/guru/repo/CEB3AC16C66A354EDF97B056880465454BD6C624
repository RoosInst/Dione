                                                                                     | array src index tmp char|                                                                                                                                      self size odd                                                                                                                                          ifTrue:[ src := ('0',self)trimBlanks asUpperCase asStream]                                                                                                                                          ifFalse:[ src := self trimBlanks asUpperCase asStream].                                                                                                                                      array := ByteArray new:(src size // 2).                                                                                                                                      index := 1.                                                                                                                                      [ src atEnd ] whileFalse:[                                                                                                                                          char := src next.                                                                                                                                          char isDigit                                                                                                                                              ifTrue:[ tmp := char digitValue * 16]                                                                                                                                              ifFalse:[ tmp := char asciiValue - 55 * 16].                                                                                                                                          char := src next.                                                                                                                                          char isDigit                                                                                                                                              ifTrue:[ tmp := tmp + char digitValue]                                                                                                                                              ifFalse:[ tmp := tmp + char asciiValue - 55].                                                                                                                                          array at:index put:(tmp min:255).                                                                                                                                          index := index + 1].                                                                                                                                      ^array                                         :SOURCE,type=instanceMethod,class=String,selector=riListStringFont,codeType=rtalkriListStringFont                                                                                                                                     "<modified:sys=GAKRE8CA,time=11/28/11 at 09:49:05 pm>   "                                                                                                                                     "<modified:sys=GAKRE8CA,time=11/28/11 at 09:48:30 pm>   "                                                                                                                                     "<modified:sys=G9TQQ5YA,time=04/15/11 at 01:09:30 pm>   "                                                                                                                                     "<modified:sys=G9TQQ5YA,time=04/15/11 at 01:04:12 pm>   "                                                                                                                                     "<modif