                                                                                                      date := Date today.                                                                                                                                      stream := WriteStream on: String new.                                                                                                                                      field1 := date month.                                                                                                                                      field2 := date day.                                                                                                                                      field3 := date year \\ 100.                                                                                                                                      field1 <= 9 ifTrue: [ stream nextPut: $0 ].                                                                                                                                      field1 printOn: stream.                                                                                                                                      stream nextPutAll: '/'.                                                                                                                                      field2 <= 9 ifTrue: [ stream nextPut: $0 ].                                                                                                                                      field2 printOn: stream.                                                                                                                                      stream nextPutAll: '/'.                                                                                                                                      field3 <= 9 ifTrue: [ stream nextPut: $0 ].                                                                                                                                      field3 printOn: stream.                                                                                                                                      stream nextPutAll: ' at '.                                                                                                                                      tmp := date hours.                                                                                                                                      tmp >= 12                                                                                                                                	ifTrue:[am := 'pm']                                                                                                                                	ifFalse:[ am := 'am'].                                                                                                                                    tmp > 12                                                                                                                                              ifTrue: [                                                                                                                                                  tmp := tmp - 12]                                                                                                                                              ifFalse: [                                                                                                                                                  (tmp = 12)                                                                                                                                                      ifFalse: [ tmp = 0 ifTrue: [tmp := 12]]].                                                                                                                                      (tmp < 10) ifTrue: [stream nextPut: $0 ].                                                                                                                                      tmp printOn: stream.                                                                                                                                      stream nextPutAll: ':'.                                                                                                                                      tmp := date minutes.                                                                                                                                      (tmp < 10) ifTrue: [stream nextPut: $0 ].                                                                                                                                      tmp printOn: stream.                                                                                                                                      stream nextPutAll: ':'.                                                                                                                                      tmp := date seconds.                                                                                                                                      (tmp < 10) ifTrue: [stream nextPut: $0 ].                                                                                                                                      tmp printOn: stream.                                                                                                                                      stream nextPutAll: ' ',am.                                                                                                                                      ^stream contents.                                        :SOURCE,type=classMethod,class=RtCodeDef,selector=methodHistory:,codeType=rtalkmethodHistory: aMethod                                                                                                                      