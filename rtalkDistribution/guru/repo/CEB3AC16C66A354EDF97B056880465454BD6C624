Map at:'class'.                                                                                                                                      	tClass := RtClass getClass:((aMap at:'class') asSymbol).                                                                                     	tClass isNil ifTrue:[self riLogMsg:'Missing class ',(aMap at:'class'). ^self].                                                                                                                                 	selector := (aMap at:'selector') riUnMap asSymbol.                                                                                                                                      type := (aMap at:1).                                                                                                                                  	newMethod := RtCompiledMethod new initialize.                                                                                                                                  	newMethod selector:selector.                                                                                                                                  	type = 'instance'                                                                                                                                  		ifTrue:[		newMethod classField:tClass.                                                                                                                                  						tClass addSelector:selector withMethod:newMethod.                                                                                                                                  						(self includesInstanceMethod: selector inClass: tClass symbol)                                                                                                                                          					ifFalse:[ self addInstanceMethod: selector toClass: tClass symbol]                                        