hat were removed now we look for adds "                                                                                                                                      aPatchLibrary tClasses do:[ :c |                                                                                                                                                          (tClasses includes:c)                                                                                                                                                              ifFalse:[tStm nextPutAll:c;cr]].                                                                                                                                      tStm isEmpty ifFalse:[rtn addKey:'New Classes' value:tStm contents].                                                                                                                                      tStm := ReadWriteStream on:''.                                                                                                                                      aPatchLibrary classMethods associationsDo:[ :c |                                                                                                                                                  c value do:[ :s |                                                                                                                                                          (cMethods includesKey:((c key asString),'-',(s asString)))                                                                                                                                                              ifFalse:[tStm nextPutAll:(s,' in class ',(c key));cr]]].                                                                                                                                      rtn addKey:'New Class Methods' value:tStm contents.                                                                                                  