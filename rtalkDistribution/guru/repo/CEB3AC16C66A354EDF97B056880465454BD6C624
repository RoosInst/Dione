isNil ifTrue: [^nil].                                                                                                                                      x := (Integer dcbFromHexString: str).                                                                                                                                      str := stream dcbNextToken.                                                                                                                                      str isNil ifTrue: [^nil].                                                                                                                                      y := (Integer dcbFromHexString: str).                                                                                                                                      ^x @ y.                                         :SOURCE,type=classMethod,class=Point,selector=fromString:,codeType=rtalkfromString: aString                                                                                                                                  "                                                                                                                                      From a string of format x@y                                                                                                                                   "                                                                                                                                      | tmp |                                                                                                                                                                                                                                                                        tmp := aString riAsArrayOfSubstringsSepBy:$@.                                                                                                                                      (tmp size = 2 ) ifFalse:[^nil].                                                                                                                                      ^((tmp at:1) trimBlanks asInteger) @ ((tmp at:2)trimBlanks asInteger)                                         :SOURCE,type=classMethod,class=RiFlags,selector=new,codeType=rtalknew                                                                                                                                          "comment"                                                                                                                                      ^self basicNew initialize                                         :SOURCE,type=classMethod,class=Message,selector=receiver:selector:,codeType=rtalkreceiver:aObject selector:aSelector                                                                                                                                          "counts the number of : to get size of arguments"                                                                                                                                      | rtn|                                                                                                                                      rtn := self new.                                                                                                                                      rtn receiver:aObject.                                                                                                                                      rtn selector:aSelector.                                                                                                                                      rtn arguments:(Array new:(aSelector asString occurrencesOf:$:)).                                                                                                                                      ^rtn                                         :SOURCE,type=classMethod,class=RtT