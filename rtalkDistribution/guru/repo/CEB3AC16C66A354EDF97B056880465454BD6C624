                                          		next := self peekNextLine.                                                                                                                                          		next size = 0 ifTrue:[self nextLine. ^rtn].                                                                                                                                          		(lastChar = $;)                                                                                                                                             		ifTrue:[(rtn riAssociationAtKey:(rtn size)) value:((rtn at:(rtn size)),((self nextLine ))).                                                                                                                                                          			lastChar := next at:(next size).]                                                                                                                                              		ifFalse:[lastChar := next at:(next size).                                                                                                                                                  				((next at:1) = $;)                                                                                                                                                      				ifTrue:[(rtn riAssociationAtKey:(rtn size)) value:((rtn at:(rtn size)),(self nextLine copyFrom:2 to:(next size)))]                                                                                                                                                      				ifFalse:[                                                                                                                                                          					((next at:1) = $,)                                                                                                                                                                          				ifTrue:[self   addString:(self nextLine) toCffMap:rtn]                                                                                                                                                                          				ifFalse:[                                                                               									(self lineStartsWithToken:next)                                                                               										ifFalse:[ self   addString:next toCffMap:rtn .                                                                               										self nextLine]                                                                                                                                                                                                                            	ifTrue:[^rtn]]].                                                                                                                                                      ]].                                                                                                                                      ^rtn                                                                              :SOURCE,type=instanceMethod,class=RtCffFile,selector=generateParagraphTree,codeType=rtalkgenerateParagraphTree                                                                                                                                                                                       "<modified:pgmr=mdr,time=06/10/14 at 01:36:56 pm>   "                                                                                       "parses the file based on the standard rules.                                                                                                                                           returns a RiMappedCollection of top level paragraphs.                                                                                                                                          current is the one being worked on,  its parent is up one"                                                                                                                                      | rtn tmp current np|                                                                                                                                      rtn := RiCffParagraphTree new initialize.                                                                                                                                      current := rtn.                                                                                                                                      self reset.                                                                                                                                      self nextLine.  " get past the time stamp "                                                                                                                                      [self atEnd] whileFalse:[                                                                                                                                             (self peek = $:)                                                                                                                                            ifTrue:[                                                                                                                                              tmp := (self nextLine) trimBlanks riMapped.                                                                                                                                              ((tmp first at:2) = $:)  " this is a push down :: "                                                                                                                                                  ifTrue:[               np := RiCffParagraphTree new initialize parent:current.                                                                                                                                                                      np name:(tmp first).                                                                                                                                                                      np addAttributes:tmp.                                                                                                                                                                      current addParagraph:np.                                                                                                                                                                      current := np.]                                                                                                                                                  ifFalse:[                                                                                                                                                    tmp first = ':CHECKSUM'  ifTrue:[[self atEnd] whileFalse:[self nextLine]. ^rtn].                                                                                                                                                    tmp first = ':END'  " this is a pop up "                                                                                                                                                      ifTrue:[current := current parent]                                                                                                                                                      ifFalse:[ " this is another paragraph to add to current parent"                                                                                                                                                                       " add a new child "                                                                                                                                                                      np := RiCffParagraphTree new initialize parent:current.                                                                                                                                                                      np name:(tmp first).                                                                                                                                                                      np addAttributes:tmp.                                                                                                                                                                      current parent addParagraph:np.                                                                                                                                                                      current := np.                                                                                                                                                                      ]]]                                                                                                                                              ifFalse:[current addLine:(self nextLine)].  " gets a line of stuff into items "                                                                                                                                                                                                                                                                                ].                                                                                                                                                                                                                                                                         self validateChecksum;close.                                                                                                                                      ^rtn                                                                               :SOURCE,type=instanceMethod,class=RtCffFile,selector=next:put:,codeType=rtalknext: anInteger put: anObject                                                                                                                                          "Answer anObject.  Put anObject to the                                                                                                                                           receiver stream anInteger number of times."                                                                                                                                      anInteger timesRepeat: [                                                                                                                                          self nextPut: anObject].                                                                                                                                      ^anObject                                         :SOURCE,type=instanceMethod,class=RtCffFile,selector=nextLine,codeType=rtalknextLine                                                                                                                                     "<modified:pgmr=mdr,time=06/10/14 at 03:48:39 pm>   "                                                                                      "computes checksum on way, returns a string"                                                                                                                                      | tmp |                                                                                                                                      nextLine isNil                                                                                                                                          ifFalse:[ tmp := nextLine.                                                                                                                                                      nextLine := 