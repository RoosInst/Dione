                                                                                                aStream := WriteStream on:                                                                                                                                                  (collection species new: self readLimit)]                                                                                                                                          ifFalse: [                                                                                                                                              aStream := WriteStream on:                                                                                                                                                  (collection copy)].                                                                                                                                      savePosition := self position.                                                                                                                                      self readLimit - 1 to: 0 by: -1 do: [ :i |                                                                                                                                          self position: i.                                                                                                                                          aStream nextPut: self next].                                                                                                                                      self position: savePosition.                                                                                                                                      ^aStream contents                                         :SOURCE,type=instanceMethod,class=Stream,selector=position:,codeType=rtalkposition: anInteger                                                                                         