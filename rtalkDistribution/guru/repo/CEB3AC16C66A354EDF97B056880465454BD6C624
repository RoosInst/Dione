lass:(cName asSymbol).                                                                                                                                                                  tClass timeStamp:pkgTime.                                                                                                                                                                  c value riIncludesKey:'modified' ifTrue:[ :a | tClass timeStamp:(a asInteger)].                                                                                                                                                                  c value riIncludesKey:'category' ifTrue:[ :a | tClass category:(a asSymbol)].]]].                                                                                                                                      tmp := sourceTree paragraphNamed:'METHODS'.                                                                                                                                      tmp isNil                                                                                                                                           ifFalse:[tmp asMultilines do:[ :c |                                                                                                                                                          self addMethodSchemaFrom:c value.                                                                                                                                                          ]].                                                                                                                                                                                                                                                                    	tmp := sourceTree paragraphsNamed:'SOURCE' .                                                                                                                                      tmp isNil          