:42:39 pm>   "                                                                                                                                          "recompile the current code"                                                                                                                                      | lib |                                                                                                                                      lib := aMap riAtKey:'library' ifAbsent:[nil].                                                                                                                                      lib isNil                                                                                                                                          ifFalse:[lib := RtCodeManager instance all detect:[ :i | i cid = lib] ifNone:[nil]].                                                                                                                                      lib isNil ifTrue:[^self].                                                                                                                                  	self promptProgress:'Compiling Package'                                                                                                                                  					progress:0                                                                                                                                  					to:(aMessage smSrc)                                                                                                                                   					channel:(aMap at:'channel').                                                                                                                                      lib recompileAll.                                                                                                                                  	self promptProgress:'Compiling Package'          