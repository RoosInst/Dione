                                                                                                                                  ifFalse:[                                                                                                                                            ( y = 0)                                                                                                                                              ifTrue:[                                                                                                                                                 ( x > 0 )                                                                                                                                                      ifTrue:[ a := 0 ]                                                                                                                                                      ifFalse:[ a := 180]                                                                                                                                                         ]                                                                                                                                              ifFalse: [                                                                                                                                               ( x abs > (y abs))                                                                                                                                                  ifTrue:[( x  > 0)                                                                                                                                                              ifFalse:[                                                                                                                                                                      ( y > 0)                                                                                                                                                                          ifTrue:[ a := 180 + ((y / x) arcTan radiansToDegrees)]                                                                                                                                                                          ifFalse:[ a :=-180 + ((y / x) arcTan radiansToDegrees)]                                                                                                                                                                          ]                                                                                                                                                              ifTrue:[a := (y / x) arcTan radiansToDegrees]                                                                                                                                                            ]                                                                                                                                                 ifFalse:[( y > 0)                                                                                                                                                              ifTrue:[ a := 90 - ((x / y) arcTan radiansToDegrees)]                                                                                                                                                              ifFalse:[a := -90 - ((x / y) arcTan radiansToDegrees)].                                                                                                                                                           ].                                                                                                                                                        ].                                                                                                                                                     ].                                                                                                                                      ^a                                         :SOURCE,type=instanceMethod,class=Point,selector=rightBottom:,codeType=rtalkrightBottom: aPoint                                                                                                                                          "Answer a Rectangle with topLeft equal to the receiver                                                                                                                                           and bottomRight equal to aPoint."                                                                                                                                      ^Rectangle leftTop: self rightBottom: aPoint                                         :SOURCE,type=instanceMethod,class=Point,selector=asString,codeType=rtalkasString                                                                                                                                          "rtn form of x@y"                                                                                                                                      ^(x asString),'@',(y asString)                                         :SOURCE,type=instanceMethod,class=Point,selector=rounded,codeType=rtalkrounded                                                                                                                                          "Answer a Point which has the receiver coordinates                                                                                                                                           rounded to integers."                                                                                                                                      ^x rounded @ y rounded                                         :SOURCE,type=instanceMethod,class=Point,selector=leftAndUp:,codeType=rtalkleftAndUp: delta                          