 := tag + 1);riListStringIndent:(aInt2 + 1)) value:self).                                                                                                                  	aCol2 add:self.                                                                                                                  	" now handle instances or array type"                                                                                                                  	index := 1.                                                                                                                  	self class isFixed                                                                                                                  		ifTrue:[                                                                                                                  			self class allInstVarNames do:[ :iv |                                                                                                                  				 tag := (self instVarAt:index) addToTree:aCol prefix:(iv ,'  ') indent:(aInt1 + 1) tag:(tag + 1) maxDepth:(aInt3 - 1 ) visited:aCol2.                                                                                                                  				index := index + 1]]                                                                                                                  		ifFalse:[                                                                                                                  			self size do:[                                                                                                                  				 tag := (self instVarAt:index) addToTree:aCol prefix:(index asString ,'  ') indent:(aInt1 + 1) tag:(tag + 1) maxDepth:(aInt3 - 1 ) visited:aCol2.                                                                                                                  				index := index + 1]].                                                                                                                                                                                                                                    	" on return send the tag"                                                                                                                  	aCol2 remove:self.                                                                                                                  	^tag                          :SOURCE,type=instanceMethod,class=Object,selector=inspect,codeType=rtalkinspect                                                                              "<modified:pgmr=mdr,time=06/11/14 at 08:33:28 pm>   "                                                                                   "comment"                                                                           	RiRmtViewInspector createInspectorOn:self                                                                           :SOURCE,type=instanceMethod,class=Object,selector=asString,codeType=rtalkasString                                                                                                                                     "<modified:pgmr=mdr,time=02/06/13 at 07:40:20 pm>   "                                                                                                                                      | stream |                                                                                                                                                                                                                                                                    	stream := RtPrintRecursion new on:(WriteStream on: (String new: 20)).                                                                                                                                      self printOn: stream.                                                                                                                              