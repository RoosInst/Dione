                                                                                                  	| tmp index lClass lMethod|                                                                                                                                      self riLogMsg:aArray.                                                                                                                                  	aArray isString                                                                                                                                  		ifFalse:[                                                                                                                                  	LaunchInfo := Dictionary new.                                                                                                                                  	index := 1.                                                                                                                                  	[index <= aArray size] whileTrue:[                                                                                                                                  		tmp := (aArray at:index) riPair.                                                                                                                                  		(tmp at:2) isEmpty                                                                                                                                  			ifTrue:[ LaunchInfo at:(tmp at:1) put:(aArray at:index + 1).                                                                                                                                  					index := index + 1]                                                                                                                                  			ifFalse:[ LaunchInfo at:(tmp at:1) put:(tmp at:2)].                                                                                                            