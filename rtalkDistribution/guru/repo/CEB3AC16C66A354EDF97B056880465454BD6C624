                            "<modified:user=Roos Inst,time=09/02/09 at 01:37:46 pm>   "                                                                                                                                          "updates attributes with first key=value lines,  and then                                                                                                                                          returns a collection of lines"                                                                                                                                      | rtn tmp prior next lastChar pos valTmp|                                                                                                                                      multilines isNil ifFalse:[^multilines].                                                                                                                                      " can have attributes as k=v,k=v                                                                                                                                          or                                                                                                                                          k=v                                                                                                                                          k=v                                                                                                                                      then the rest is                                                                                                                                      v,k=v,k=v etc"                                                                                                                                      multilines := RiCffMappedCollection new.                                                                                                                                      lines isEmpty ifTrue:[^multilines].                                                                                                                                      " when called could be attributes or multilines,  first multline is                                                                                                                                        when the first value has no = "                                                                                                                                      lines do:[ :s |                                                                                                                                       s isEmpty                                                                                                                                        ifFalse:[                                                                                                                                          " handle extension chars ; "                                                                                                                                          ((lastChar = $;) | ((s at:1) = $;))                                                                                                                                              ifFalse:[next := s riMapped.                                                                                                                                                        valTmp isNil                                                                                                                                                           ifFalse:[ prior collection last value:( valTmp contents).                                                                                                                                                                     valTmp := nil]]                                                                                                                                              ifTrue:[ next := ''.                                                                                                                                                       valTmp isNil                                                                                                                                                              ifTrue:[ valTmp := ReadWriteStream on:(prior collection last value).                                                                                                                                                                       valTmp setToEnd].                                                                                                                                                  (lastChar = $;)                                                                                                                                                      ifTrue:[ valTmp nextPutAll:s]                                                                                                                                                      ifFalse:[valTmp nextPutAll:(s copyFrom:2 to:( s size))]].                                                                                                                                          lastChar := s last.  " used to check for continuation of value "                                                                                                                                          next isEmpty                                                                                                                                                ifFalse:[                                                                                                                                                  (next associationAt:1) key = ''                                                                                                                                                         ifTrue:[ prior isNil ifFalse:[multilines addKey:(prior at:1) value:prior].                                                                                                                                                                  prior := next.  "start a new multlline"]                                                                                                                                                      ifFalse:[prior isNil                                                                                                                                                                  ifTrue:[attributes addAll:next]                                                                                                                                                                  ifFalse:[prior addAll:next.  " continuation"]                                                                                                                                                                              ]]]].                                                                                                                                      prior isNil                                                                                                                                           ifFalse:[                                                                                                                                              valTmp isNil                                                                                                                                                   ifFalse:[prior collection last value:( valTmp contents)] .                                                                                                                                              multilines addKey:(prior at:1) value:prior].                                                                                                                                      ^multilines                                         :SOURCE,type=instanceMethod,class=RiCffParagraphTree,selector=name,codeType=rtalkname                                                                                                                                          "this is a single word"                                                                                                                                      ^name                                         :SOURCE,type=instanceMethod,class=RiCffParagraphTree,selector=childNamed:,codeType=rtalkchildNamed:aString                                                                                                                                          "comment"                                                                                                                                      ^children at:aString ifAbsent:[nil]                                         :SOURCE,type=instanceMethod,class=RiCffParagraphTree,selector=multiLines,codeType=rtalkmultiLines                                                                                                                                     "<modified:sys=G9TQQ5YA,time=02/18/10 at 01:10:01 pm>   "                                                                                                                                     "<modified:user=Roos Inst,time=01/03/08 at 02:28:13 pm>   "                                                                                                                                          "multilines is a mapped collection, associations"                                                                                                                                      ^multilines                                         :SOURCE,type=instanceMethod,class=RiCffParagraphTree,selector=paragraphsDo:,codeType=rtalkparagraphsDo:aBlock                       