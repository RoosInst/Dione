                                                          self riLogMsg:'sending sync SM'.                                                                                                                                              self printToConsole.                                                                                                                                              ].                                                                                       	" returns a 3 element array , or nil if an error"                                                                                                                                     	reply := RiSystemMessageManager msgManager                                                                                       				sendMessageSync:msg id:aSyncId.                                                                                      	reply isNil                                                                                      		ifTrue:[header at:2 put:1]                                                                                      		ifFalse:[header := reply at:1.                                                                                             				parameters := reply at:3.                                    				position := 0.                                                                                                                     				destination := reply at:2.                                                                                  				].                       :SOURCE,type=instanceMethod,class=RtSystemMessage,selector=createReplySmFrom:,codeType=rtalkcreateReplySmFrom:aSrc                                                                                             "<modified:pgmr=mdr,time=07/31/15 at 02:49:04 pm>   "                                                                        "creates a reply message from this ones routing"    	  