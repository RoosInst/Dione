                                                                                                                                                                         			].].                                                                                                                                                                                                                                                                        oLine < oLines size                                                                                                                                          ifTrue:[ [oLine <= oLines size]                                                                                                                                               whileTrue:[diff add:(oLines at:oLine).                                                                                                                                  							colors add:2.                                                                                                                                                            oLine := oLine + 1]].                                                                                                                                       cLine < cLines size                                                                                                                                          ifTrue:[ [cLine <= cLines size]                                                                                                                                               whileTrue:[diff add:(cLines at:cLine).                                                                                                                                  							colors add:5.                                                                                                                                                            cLine := cLine + 1]].                    