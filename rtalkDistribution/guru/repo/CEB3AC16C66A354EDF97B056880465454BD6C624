                                          	 		nLine := inStrm nextLine.                                                                                                                                  	 		(nLine includes:$") or:[ nLine includes:$|]]]                                                                                                                                   		whileFalse:[ outStrm nextPutAll:nLine; cr].                                                                                                                                  	" if we are at the end then assume it should be at line 2 "                                                                                                                                  	inStrm atEnd                                                                                                                                  		ifTrue:[                                                                                                                                   			nLine := nil.                                                                                                                                     			inStrm := ReadStream on:aString.                                                                                                                                      		outStrm := ReadWriteStream on:''.                                                                                                                                      		outStrm nextPutAll:(inStrm nextLine).                                                                                                                                      		outStrm cr].                                                                                                                                  	" now insert the time tag "                                                                                                                              