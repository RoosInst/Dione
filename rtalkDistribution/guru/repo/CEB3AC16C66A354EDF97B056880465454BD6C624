                                                                                                                            | trans tmp ident thread dest msg|                                                                                                                                      trans := RiSystemMessageManager msgManager handlerNamed:'TRANSCRIPT'.                                                                                                                                      trans isNil ifTrue:[Object vmError:aString. ^self].                                                                                                                                      trans subscribers isEmpty ifTrue:[Object vmError:aString. ^self].                                                                                                                                      DebugCount isNil ifTrue:[DebugCount := 0].                                                                                                                                      tmp := self new initialize.                                                                                                                                      ident := tmp identifier.                                                                                                                                      tmp process:(thread := RtThread currentThread) resumable:aBool title:aString.                                                                                                                                      tmp threadName:'RtDebug',((DebugCount := DebugCount + 1) asString).                                                                                                                                      thread name:(tmp threadName).                                                                                                                                  	tmp source:(thread smMessage).                                                                                                                                      RiSystemMessageManager msgManager addHandler:ident handler:tmp.                                                                                                                                      dest := trans subscribers any.                                                                                                                                          msg := RiSystemMessageManager createSmTo:(dest value at:2) action:'createSubscriber' id:'1' from:ident.                                                                                                                                                                                           msg ,'^+view=Debugger'.                                                                                                                                      msg sendMessageAsync.                                                                                                                            "thread wait"                                                                                                                                      tmp suspendThread.                    :SOURCE,type=classMethod,class=RiRmtViewDebugger,selector=refreshSubscribers:,codeType=rtalkrefreshSubscribers:aThdName                                                                                                                                     "<modified:sys=GAKRE8CA,time=11/12/11 at 06:37:46 pm>   "                                                                                                                                     "<modified:sys=GAKRE8CA,time=11/12/11 at 06:22:22 pm>   "                                                                                                                                          "indicates that all subscribers to this thread name should be repainted"               