       quot := $".                                                                                                                                      delimiter := aCharacter.                                                                                                                                      answer := OrderedCollection new.                                                                                                                                      startIndex := 1.                                                                                                                                      size := self size.                                                                                                                                      size = 1  " for single char strings if only one is sep then no result"                                                                                                                                          ifTrue:[(self at:1) = aCharacter                                                                                                                                                      ifTrue:[^answer asArray]                                                                                                                                                      ifFalse:[answer add:self.                                                                                                                                                                  ^answer asArray]].                                                                                                                                                                                                                                                                                                [ startIndex <= size ] whileTrue: [                                                                                                                      