                                                    delimiter := $,.                                                                                                                                      answer := RiCffMappedCollection new.                                                                                                                                      startIndex := 1.                                                                                                                                      size := self size.                                                                                                                                      size = 1  " for single char strings if only one is sep then no result"                                                                                                                                          ifTrue:[(self at:1) = $,                                                                                                                                                      ifTrue:[^answer]                                                                                                                                                      ifFalse:[answer add:(self riKeyValue).                                                                                                                                                                  ^answer]].                                                                                                                                                                                                                                                                                                [ startIndex <= size ] whileTrue: [                                                                                                                                          [startIndex <= size and: [ ( c0 := self at: startIndex ) = delimiter | (c0 = quot)]] whileTrue:[                                                                                                                                              c0 = $,                                                                                                                                                   ifTrue:[                                                                                                                                                       delimiter := $,.                                                                                                                                                      (startIndex = 1) ifFalse:[answer add:( '' riKeyValue)]]                                                                                                                                                  ifFalse:[ delimiter := quot ].                                                                                                                                              startIndex := startIndex + 1].                                                                                                                                          stopIndex := startIndex.                                                                                                                                          [ stopIndex <= size and: [ ( self at: stopIndex ) ~= delimiter ] ]                                                                                                                                              whileTrue: [ stopIndex := stopIndex + 1 ].                                                                                                                                          stopIndex >= startIndex                                                                                                                                              ifTrue: [                                                                                                                                                      ((stopIndex >= size) & (startIndex = stopIndex)) "gets an extra empty string if double sep is last"                                                                                                                                                          ifFalse:[answer add: (( self copyFrom: startIndex to: stopIndex - 1 )riKeyValue)].                                                                                                                                                          delimiter := $, ].                                                                                                                                          startIndex := stopIndex + 1.                                                                                                                                           (startIndex < size and:[( self at: stopIndex ) = quot])                                                                                                                                               ifTrue:[startIndex := startIndex + 1]                                                                                                                                      ].                                                                                                                                      ^answer                                         :SOURCE,type=instanceMethod,class=String,selector=diffWith:,codeType=rtalkdiffWith:aString                                                                                                                                     "<modified:pgmr=mdr,time=02/19/13 at 11:54:05 am>   "                                                                                                                                          "generate a colored list of differences"                                                                                                                                      | aClass aMethod dict hist diff lastKey old current cLine oLine cLines oLines oMatch cMatch oTmp cTmp ver tmp selectedMethod|                                                                                                                                      " now do the diff "                                                                                                                                      old  := ReadStream on:aString.                                                                                                                                      current := ReadStream on:self.                                                                                                                                      diff := OrderedCollection new.                                                                                                                                      diff add:('Lines in old and current' riListStringColor:#Violet).                                                                                                                                      diff add:('Lines in current but not in old' riListStringColor:#Red).                                                                                                                                      diff add:('Lines in old and not in current' riListStringColor:#DarkGray).                                                                                                                                      cLines := OrderedCollection new.                                                                                                                                      [current atEnd] whileFalse:[tmp := current nextLine.                                                                                                                                                                        tmp isEmpty ifFalse:[ cLines add:tmp]].                                                                                                                                      oLines := OrderedCollection new.                                                                                                                                      [old atEnd] whileFalse:[tmp := old nextLine.                                                                                                                                                                  tmp isEmpty ifFalse:[oLines add:tmp]].                                                                                                                                      cLine := 1.                                                                                                                                      oLine := 1.                                                                                                                                      [(cLine <= cLines size) & (oLine <= oLines size)] whileTrue:[                                                                                                                                      (((cLines at:cLine) trimBlanks) = ((oLines at:oLine) trimBlanks))                                                                                                                                              ifTrue:[diff add:((cLines at:cLine)riListStringColor:#Violet).                                                                                                                                                          cLine := cLine + 1.                                                                                                                                                      oLine := oLine + 1]                                                                                                                                              ifFalse:[ " find the next match of any following line in c into o "                                                                                                                                                      cTmp := cLine.                                                                                                                                                      oTmp := oLine.                                                                                                                                                      cMatch := nil.                                                                                                                                                      [ (cTmp <= cLines size) & (cMatch isNil)] whileTrue:[                                                                                                                                                          oTmp := oLine.                                                                                                                                                          [(oTmp <= oLines size)  & (cMatch isNil)] whileTrue:[                                                                                                                                                                  (((cLines at:cTmp)trimBlanks) = ((oLines at:oTmp)trimBlanks))                                                                                                                                                                      ifTrue:[cMatch := cTmp]                                                                                                                                                                      ifFalse:[oTmp := oTmp + 1]].                                                                                                                                                          cTmp := cTmp + 1].                                                                                                                                                       "cMatch and oTmp are the next alignment points so write them out "                                                                                                                                                  [oLine < oTmp] whileTrue:[diff add:((oLines at:(oLine))riListStringColor:#DarkGray).                                                                                                                                                                                              oLine := oLine + 1].                                                                                                                                                  cMatch isNil ifFalse:[[cLine < cMatch] whileTrue:[diff add:((cLines at:(cLine))riListStringColor:#Red).                                                                                                                                                                                              cLine := cLine + 1]].                                                                                                                                          ].].                                                                                                                                      oLine < oLines size                                                                                                                                          ifTrue:[ [oLine <= oLines size]                                                                                                                                               whileTrue:[diff add:((oLines at:oLine)riListStringColor:#DarkGray).                                                                                                                                                            oLine := oLine + 1]].                                                                                                                                       cLine < cLines size                                                                                                                                          ifTrue:[ [cLine <= cLines size]                                                                                                                                               whileTrue:[diff add:((cLines at:cLine)riListStringColor:#Red).                                                                                                                                                            cLine := cLine + 1]].                                                                                                                                       ^diff                                         :SOURCE,type=instanceMethod,class=String,selector=riListStringContents:,codeType=rtalkriListStringContents:aString                                                                                                                                     "<modified:pgmr=mdr,time=08/24/12 at 09:52:56 am>   "                                                                                                                                     "<modified:pgmr=mdr,time=08/24/12 at 09:47:44 am>   "                                                                                                                                          "replace the string portion                                                                                                                                              "                                                                                                                                      | start len|                                                                                                                                      self riListStringTest ifFalse:[^self].                                                                                                                                      ^aString riListString riListHeader:(self riListHeader)          