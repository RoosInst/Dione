alkprintOn: aStream                                                                                                                                     "<modified:pgmr=mdr,time=08/15/12 at 10:58:33 pm>   "                                                                                                                                     "<modified:pgmr=mdr,time=08/15/12 at 10:54:40 pm>   "                                                                                                                                     "<modified:sys=GAKRE8CA,time=12/12/11 at 10:44:07 am>   "                                                                                                                                     "<modified:sys=GAKRE8CA,time=08/19/11 at 09:14:01 pm>   "                                                                                                                                          "Append a string representing the receiver date                                                                                                                                           to aStream in the form specified by the current                                                                                                                                           system date format."                                                                                                                                      | tmp |                                                                                                                                      month <= 9 ifTrue: [ aStream nextPut: $0 ]. "month "                                                                                                                                      month printOn: aStream.                                                                                                                                      aStream nextPutAll:'/'.                                                                                                                                      day <= 9 ifTrue: [ aStream nextPut: $0 ].  " day "                                                                                                                                      day printOn: aStream.                                                                                                                                      aStream nextPutAll:'/'.                                                                                                                                        "year "                                                                                                                                      year printOn: aStream.                                                                                                                                      aStream nextPutAll:' '.                                                                                                                                      tmp := self hours.                                                                                                                                      (tmp < 10) ifTrue: [aStream nextPut: $0 ].                                                                                                                                      tmp printOn: aStream.                                                                                                                                      aStream nextPutAll: ':'.                                                                                                                                      tmp := seconds - (self hours * 3600) // 60.                                                                                                                                      (tmp < 10) ifTrue: [aStream nextPut: $0 ].                                                                                                                                      tmp printOn: aStream.                                                                                                                                      aStream nextPutAll: ':'.                                                                                                                                      tmp := seconds - (self hours * 3600) \\ 60.                                                                                                                                      (tmp < 10) ifTrue: [aStream nextPut: $0 ].                                                                                                                                      tmp printOn: aStream.                                         :SOURCE,type=instanceMethod,class=Date,selector=day:month:year:,codeType=rtalkday:aInt1 month:aInt2 year:aInt3                                                                                                                                     "<modified:sys=GAKRE8CA,time=12/16/11 at 08:02:36 pm>   "                                                                                                                                     "<modified:sys=GAKRE8CA,time=12/16/11 at 08:01:35 pm>   "                                                                                                                                          "initialize self"                                                                                                                                      day := aInt1.                                                                                                                                      month := aInt2.                                                                                                                                      year := aInt3.                                                                                                                            