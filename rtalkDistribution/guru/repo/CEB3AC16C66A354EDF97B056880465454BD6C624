                                                                                     aLibrary isNil ifTrue:[^aSelector asString riListStringColor:#Black].                                                                                                                                      " check if in multiple libs "                                                                                                                                      mult := (RtCodeManager instance libsIncludingClassMethod:aSelector inClass: aClass symbol) size.                                                                                                                                      this := aLibrary includesClassMethod:aSelector inClass: aClass symbol.                                                                                                                                    col := #Black.                                                                                                                                          (this & (mult > 1))                                                                                                                                              ifTrue: [ col := #Magenta ]                                                                                                                                          ifFalse: [ this                                                                                                                                              ifTrue: [ col := #Red ]                                                                                                                                          ifFalse: [ (mult > 0)                                                                                                                                              ifTrue: [ col := #LtGray ]                                                                                                                                          ifFalse: [                                                                                                                                              col := #DarkGray                                                                                                                                          ] ] ].                                                                                                                                      ^aSelector asString riListStringColor:col                              :SOURCE,type=instanceMethod,class=RiRmtViewCode,selector=confirmAddAllMethods:message:,codeType=rtalkconfirmAddAllMethods:aMap message:aMessage                                                                                                                                     "<modified:pgmr=mdr,time=08/16/16 at 07:48:59 pm>   "                         "after confirm adds all methods to the package"                                                                                                                                      | sel msg cookie lib sClass|                                                                                                                                       cookie := aMap riAtKey:'cookie' ifAbsent:[nil].                                                                                                                                      cookie isNil                                                                                                                                          ifTrue:[  " first time so send the dialog"                                                                                                                            				lib := self helperGetLib:aMap.                                                                                                                                  				lib isNil ifTrue:[^self].                                                                