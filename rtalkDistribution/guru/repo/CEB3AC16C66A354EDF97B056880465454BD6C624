  iMethods  := Dictionary new.                                                                                                                                      sourceTree paragraphsDo:[ :a |                                                                                                                                          a name = 'CLASSES'                                                                                                                                               ifTrue:[a multiLinesDo:[ :c |                                                                                                                                                          tClasses add:((c riAtKey:'class') asSymbol).                                                                                                                                                          (aPatchLibrary includesClass:((c riAtKey:'class') asSymbol))                                                                                                                                                              ifFalse:[rtn addKey:('Removed class ',(c riAtKey:'class')) value:(c riAtKey:'class')]]].                                                                                                                                          a name = 'CODE'                                                                                                                                               ifTrue:[                                                                                                                                                  tmp := a attributes riAtKey:'type'.                                                                                                                                                   tmp = 'classMethod'                                                                                                                                                      ifTrue:[