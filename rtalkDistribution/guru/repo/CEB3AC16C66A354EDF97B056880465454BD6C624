                                                                                                                 						ifTrue:[lMethod := (tmp at:2) asSymbol.                                                                                                                                  								(lClass respondsTo:lMethod)                                                                                                                                  									ifTrue:[ lClass perform:lMethod]]                                                                                                                                  						ifFalse:[lMethod := ((tmp at:2),':')asSymbol.                                                                                                                                  								(lClass respondsTo:lMethod)                                                                                                                                  									ifTrue:[ lClass perform:lMethod with:( tmp copyFrom:3 to:(tmp size))]]                                                                                                                                  					].                                                                                                                                  	(LaunchInfo includesKey:'start')                                                                                                                                  		ifTrue:[ tmp := (LaunchInfo at:'start') riAsArrayOfSubstringsSepBy:$:.                                                                                                                                  					tmp size = 2  ifTrue:[RtScriptDef executeScript:(tmp at:1) page:(tmp at:2)]                                                                                                                                  					].         :SOURCE,type=classMethod,class=RtSystem,selector=objectFromString:,codeType=rta