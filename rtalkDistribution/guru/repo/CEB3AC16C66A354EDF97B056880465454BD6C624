                          of the receiver to aStream."                                                                                                                                      aStream cr.                                                                                                                                      'name ' printOn: aStream.                                                                                                                                      self name printOn: aStream.                                                                                                                                      attributes isEmpty                                                                                                                                          ifFalse:[                                                                                                                                              attributes associationsDo:[ :a |                                                                                                                                              aStream cr.                                                                                                                                               a printOn:aStream]].                                                                                                                                      aStream cr;cr.                                                                                                                                          self contentsAsString printOn: aStream                                         :SOURCE,type=instanceMethod,class=RiCffParagraphTree,selector=branches,codeType=rtalkbranches                                                                                                                                     "<modified:user=Roos Inst,time=01/04/08 at 06:56:47 pm>   "                            