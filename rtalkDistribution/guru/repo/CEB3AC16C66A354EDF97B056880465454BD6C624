                                                                                                        [self atEnd]                                                                                                                                          whileFalse: [aBlock value: self next]                                         :SOURCE,type=instanceMethod,class=Stream,selector=nextWord,codeType=rtalknextWord                                                                                                                                          "Answer a String containing the next word in the                                                                                                                                           receiver stream.  A word starts with a letter,                                                                                                                                           followed by a sequence of letters and digits."                                                                                                                                      | first aChar |                                                                                                                                      [self atEnd ifTrue: [^nil].                                                                                                                                      self next isAlphaNumeric]                                                                                                                                          whileFalse: [].  "skip separators"                                                                                                                                      first := self position.                                                                                                                                      [self atEnd                                                                                                                                          ifTrue: [^self copyFrom: first to: self position].                                                                                                                                       (aChar := self next) isAlphaNumeric]                                                                                                                                          whileTrue: [].                                                                                                                                      self backupOver: aChar.                                                                                                                                      ^self copyFrom: first to: self position                                         :SOURCE,type=instanceMethod,class=Stream,selector=size,codeType=rtalksize                                                                                                                                          "Answer the size of (number of objects in) the                                                                                                                                           receiver stream."                                                                                                                                      ^self readLimit                                         :SOURCE,type=instanceMethod,class=Stream,selector=countBlanks,codeType=rtalkcountBlanks                                                                                                                                     "<modified:sys=GAKRE8CA,time=05/11/11 at 10:12:58 am>   "                                                                                                                                          "Skip over blank and tab characters.  Answer the                                                                                                                                           number of character positions skipped, counting                                                                                                                                           1 for blanks and 4 for tabs."                                                                                                                                      | answer char |                                                                                                                                      answer := 0.                                                                                                                                      [self atEnd]                                                                                                                                          whileFalse: [                                                                                                                                              char := self next.                                                                                                                                              char == $                                                                                                                                                   ifTrue: [answer := answer + 1]                                                                                                                                                  ifFalse: [                                                                                                                                                      char == (Character tab)                                                                                                                                                          ifTrue: [answer := answer + 4]                                                                                                                                                          ifFalse: [                                                                                                                                                              self backupOver: char.                                                                                                                                                              ^answer]]].                                                                                                                                      ^answer                                         :SOURCE,type=instanceMethod,class=Stream,selector=position,codeType=rtalkposition                                                                                                                                          "Answer the current receiver stream position."                                                                                                                                      ^position                                         :SOURCE,type=instanceMethod,class=Stream,selector=skipTo:,codeType=rtalkskipTo: anObject                                                                                                                                          "Advance the receiver position beyond the next                                                                                                                                           occurrence of anObject, or if none, to the end of                                                                                                                                           stream.  Answer true if anObject occurred, else                                                                                                                                           answer false."                                                                                                                                      [self atEnd]                                                                                                                                          whileFalse: [                                                                                                                                              self next = anObject                                                                                                                                                  ifTrue: [^true]].                                                                                                                                      ^false                                         :SOURCE,type=instanceMethod,class=Stream,selector=peek,codeType=rtalkpeek                                                                                                                                          "Answer the next object in the receiver stream                                                                                                                                           without advancing the stream position.  If the                                                                                                                                           stream is positioned at the end, answer nil."                                                                                                                                      | anObject |                                                                                                                                      self atEnd                                                                                                                                          ifTrue: [^nil]                                                                                                                                          ifFalse: [                                                                                                                                              anObject := self next.                                                                                                                                              self backupOver: anObject.                                                                                                                                              ^anObject]                                         :SOURCE,type=instanceMethod,class=Stream,selector=isStream,codeType=rtalkisStream                                                                                                                                          "Answer true if receiver is a kind of Stream."                                                                                                                                      ^true                                         :SOURCE,type=instanceMethod,class=Stream,selector=peekFor:,codeType=rtalkpeekFor: anObject                                                                                                                                          "Answer true if the next object to be accessed in                                                                                                                                           the receiver stream equals anObject, else answer                                                                                                                                           false.  Only advance the stream position if the                                                                                                                                           answer is true."                                                                                                                                      | nextObject |                                                                                                                                      self atEnd                                                                                                                                          ifTrue: [^false]                                                                                                                                          ifFalse: [                                                                                                                                              anObject = (nextObject := self next)                                                                                                                                                  ifTrue: [^true]                                                                                                                                                  ifFalse:[                                                                                                                                                      self backupOver: nextObject.                                                                                                                                                      ^false]].                                         :SOURCE,type=instanceMethod,class=Stream,selector=skip:,codeType=rtalkskip: anInteger                                                                                                                                          "Increment the position of the                                                                                                                                           receiver by anInteger."                                                                                                                                      | newPosition |                                                                                                                                      self position:                                                                                                                                          ((self position + anInteger                                                                                                                                              min: self readLimit) max: 0)                                         :SOURCE,type=instanceMethod,class=Stream,selector=readLimit,codeType=rtalkreadLimit                                                                                                                                          "Private - Answer the size of                                                                                                                                           the receiver collection."                                                                                                                                      ^readLimit                                         :SOURCE,type=instanceMethod,class=Stream,selector=lineDelimiter:,codeType=rtalklineDelimiter: aCharacter                                                                                                                                          "Change the line delimiter character                                                                                                                                           to aCharacter.  Ignore for non-file                                                                                                                                           streams."                                         :SOURCE,type=instanceMethod,class=Stream,selector=upTo:,codeType=rtalkupTo: anObject                                                                                                                                     "<modified:sys=GAKRE8CA,time=04/25/11 at 03:18:29 pm>   "                                                                                                                                          "Answer the collection of objects from the receiver                                                                                        